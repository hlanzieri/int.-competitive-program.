#include <iostream>

using namespace std;

int main()
{
    int X[3][3], soma[3] ={0};
    for(int i=0; i<3; i++){
        for(int j=0; j<3; j++){
            cin >> X[i][j];
        }
    }
    for(int i=0; i<3; i++){
        soma[0]+=X[0][i];
        soma[1]+=X[1][i];
        soma[2]+=X[2][i];
    }
    cout << "Linha 0: " << soma[0] << endl;
    cout << "Linha 1: " << soma[1] << endl;
    cout << "Linha 2: " << soma[2] << endl;
    return 0;
}
